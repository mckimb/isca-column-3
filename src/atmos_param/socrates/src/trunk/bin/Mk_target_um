#
# Full list of UM utilities
#
TARGET_UM: spec_nml nml_spec

spec_nml: spec_nml.o out_nml.o radlib.$(LIBSUFFIX)
	$(LINK) -o spec_nml spec_nml.o out_nml.o \
	   radlib.$(LIBSUFFIX)

nml_spec: nml_spec.o def_um_nml.o radlib.$(LIBSUFFIX)
	$(LINK) -o nml_spec nml_spec.o def_um_nml.o \
	   radlib.$(LIBSUFFIX)
